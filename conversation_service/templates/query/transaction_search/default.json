{
  "template_name": "transaction_search_default",
  "description": "Template par d√©faut pour recherche de transactions (fallback)",
  "target_intention": "TRANSACTION_SEARCH.default",
  "parameters": {
    "user_id": "{{user_id}}",
    "filters": {
      "transaction_type": "{{transaction_type}}"
    },
    "sort": [
      {"date": {"order": "desc"}}
    ],
    "page_size": 50
  },
  "parameter_mappings": {
    "user_id": {
      "required": true,
      "type": "integer",
      "source": "context.user_id"
    },
    "transaction_type": {
      "required": false,
      "type": "string",
      "source": "entities.transaction_type",
      "allowed_values": ["debit", "credit"],
      "default": ""
    }
  },
  "filter_cleanup": {
    "remove_null_values": true,
    "remove_empty_objects": true
  },
  "optimizations": {
    "cache_duration": "15m",
    "elasticsearch_routing": "user_id"
  }
}
